{
  "name": "postcode-scraper-app",
  "version": "1.0.0",
  "description": "Web application for scraping Malaysian postcode data with CAPTCHA handling",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "echo 'No build step required'",
